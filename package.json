{
  "name": "launchdarkly-deployment-events-action",
  "version": "0.0.1",
  "description": "Send deployment events to LaunchDarkly",
  "main": "index.js",
  "scripts": {
    "build": "ncc build src/index.js --license LICENSE",
    "docs": "action-docs -u --no-banner && npm run prettier:write:path README.md",
    "lint": "eslint src/*.js",
    "prettier": "prettier",
    "prettier:check": "prettier -c ./",
    "prettier:write": "prettier -w ./",
    "prettier:write:path": "prettier -w",
    "test": "jest --testMatch 'src/**/*.test.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/launchdarkly/deployment-events-action.git"
  },
  "keywords": [],
  "author": "",
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://github.com/launchdarkly/deployment-events-action/issues"
  },
  "homepage": "https://github.com/launchdarkly/deployment-events-action#readme",
  "dependencies": {
    "@actions/core": "1.10.0"
  },
  "devDependencies": {
    "@babel/core": "7.22.5",
    "@babel/preset-env": "7.22.5",
    "@vercel/ncc": "0.36.1",
    "action-docs": "1.2.0",
    "babel-jest": "29.5.0",
    "eslint": "8.43.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.5.0",
    "prettier": "2.8.8"
  }
